{
  "name": "react-es6-webpack-minimal-starter-template",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Jose Quinto Zamora",
    "email": "quintorel@hotmail.com",
    "url": "https://blog.josequinto.com"
  },
  "scripts": {
    "start": "set NODE_ENV=development && webpack-dev-server --open --config ./webpack/webpack.config.dev.js",
    "dev": "set NODE_ENV=development && webpack --progress --config ./webpack/webpack.config.dev.js",
    "build": "set NODE_ENV=production && webpack --progress --config ./webpack/webpack.config.prod.js",
    "stats": "set NODE_ENV=production && webpack --progress --config ./webpack/webpack.config.stats.js --profile --json > ./webpack/stats/stats.json"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "3.0.0",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.5.3",
    "react-dev-utils": "3.1.1",
    "react-hot-loader": "3.0.0-beta.7",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "webpack": "3.5.4",
    "webpack-dev-server": "2.7.1",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "dependencies": {
    "core-js": "2.5.0",
    "react": "15.6.1",
    "react-dom": "15.6.1"
  }
}
